{
    "name": "starter-repo",
    "version": "0.1.0",
    "description": "Next.js Starter Repo",
    "author": "Ayush Bhatt",
    "license": "MIT",
    "private": true,
    "engineStrict": true,
    "engines": {
        "node": ">=18.19.0",
        "pnpm": ">=8.7.6"
    },
    "scripts": {
        "dev": " next dev -p 9500",
        "dev-turbo": " next dev -p 9500 --turbo",
        "build": "next build",
        "prod": "clear; rm -rf .next && pnpm run build && next start -p 9500",
        "check:types": "clear; tsc --noEmit",
        "husky": "chmod ug+x .husky/* && husky install .config/husky && pnpx husky add .husky/commit-msg 'npx commitlint --edit $1'",
        "prepare": "husky install"
    },
    "dependencies": {
        "@nextui-org/react": "^2.4.0",
        "@tanstack/react-query": "^5.40.0",
        "@vercel/analytics": "^1.3.1",
        "@vercel/speed-insights": "^1.0.11",
        "axios": "^1.7.2",
        "clsx": "^2.1.1",
        "dayjs": "^1.11.11",
        "framer-motion": "^11.2.9",
        "immer": "^10.1.1",
        "next": "14.2.3",
        "next-easy-middlewares": "^1.0.10",
        "nextjs-toploader": "^1.6.12",
        "react": "^18.3.1",
        "react-dom": "^18.3.1",
        "react-hot-toast": "^2.4.1",
        "react-icons": "^5.2.1",
        "react-loader-spinner": "^6.1.6",
        "react-spinners": "^0.13.8",
        "rooks": "^7.14.1",
        "semver": "^7.6.2",
        "server-only": "^0.0.1",
        "tailwindcss-animate": "^1.0.7",
        "ua-parser-js": "^1.0.38",
        "zod": "^3.23.8",
        "zustand": "^4.5.2"
    },
    "devDependencies": {
        "@commitlint/cli": "^19.3.0",
        "@commitlint/config-conventional": "^19.2.2",
        "@commitlint/types": "^19.0.3",
        "@tanstack/react-query-devtools": "^5.40.0",
        "@types/node": "^20.12.13",
        "@types/react": "^18.3.3",
        "@types/react-dom": "^18.3.0",
        "@types/ua-parser-js": "^0.7.39",
        "autoprefixer": "^10.4.19",
        "cssnano": "^7.0.1",
        "husky": "^9.0.11",
        "is-ci": "^3.0.1",
        "next-compose-plugins": "^2.2.1",
        "postcss": "^8.4.38",
        "sass": "^1.77.3",
        "tailwind-merge": "^2.3.0",
        "tailwindcss": "^3.4.3",
        "typescript": "^5.4.5"
    }
}
